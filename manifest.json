  "name": "FocusTab",
  "description": "A productivity-focused new tab with tasks, quotes, and Pomodoro timer.",
  "author": "vishnu",
  "version": "1.0.0",

  "chrome_url_overrides": {
    "newtab": "dist/index.html"
